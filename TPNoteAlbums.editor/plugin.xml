<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated albums -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="albums.presentation.AlbumsModelWizardID"
            name="%_UI_AlbumsModelWizard_label"
            class="albums.presentation.AlbumsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/AlbumsModelFile.gif">
         <description>%_UI_AlbumsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated albums -->
      <editor
            id="albums.presentation.AlbumsEditorID"
            name="%_UI_AlbumsEditor_label"
            icon="icons/full/obj16/AlbumsModelFile.gif"
            extensions="albums"
            class="albums.presentation.AlbumsEditor"
            contributorClass="albums.presentation.AlbumsActionBarContributor">
      </editor>
   </extension>

</plugin>
